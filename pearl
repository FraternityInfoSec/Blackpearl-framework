#!/bin/env python
import os,sys
import time
import random
from urllib.request import urlopen
w= '\x1b[00m'
r= '\x1b[91m'
F = '\033[91m'
G = '\033[92m'
os.system("run")
os.system("clear")
def delay_print(s):
    for c in s:
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(1.5 / 10)
def sprint(s):
        for c in s +'\n':
                sys.stdout.write(c)
                sys.stdout.flush()
                time.sleep(0.1  / 100)
